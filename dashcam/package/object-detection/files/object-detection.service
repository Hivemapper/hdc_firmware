[Unit]
Description=Object Detection
After=network.target

[Service]

User=root
WorkingDirectory=/opt/dashcam/bin
ExecStart=cpulimit -l 230 python3 detect_hdc.py
Restart=always

StandardOutput=append:/mnt/data/object-detection.log
StandardError=append:/mnt/data/object-detection.log

[Install]
WantedBy=multi-user.target
